---
title: Contact Me
layout: layouts/_regular.njk
---

<h1>{{ title }}</h1>

<form name="contact" method="POST" data-netlify="true">
    <label for="name">
        Name: <input type="text" id="name" name="name"/>
    </label>   

    <label for="email">
        Email: <input type="text" id="email" name="email"/>
    </label>
    
    <button type="submit">Send</button>
</form>
